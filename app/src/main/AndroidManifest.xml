<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wrl.viewtest" >

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".LauncherActivity"
            android:label="@string/title_activity_launcher" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ArcMenuActivity"
            android:label="@string/title_activity_arc_menu" >
        </activity>
        <activity
            android:name=".NormalSlideActivity"
            android:label="@string/title_activity_normal_slide" >
        </activity>
        <activity
            android:name=".QQSlideActivity"
            android:label="@string/title_activity_qqslide" >
        </activity>
        <activity
            android:name=".AnimateProgressActivity"
            android:label="@string/title_activity_animate_progress" >
        </activity>
        <activity
            android:name=".OpenGLESActivity"
            android:label="@string/title_activity_open_gles" >
        </activity>
        <activity
            android:name=".RatioImageViewActivity"
            android:label="@string/title_activity_ratio_image_view" >
        </activity>
    </application>

</manifest>
